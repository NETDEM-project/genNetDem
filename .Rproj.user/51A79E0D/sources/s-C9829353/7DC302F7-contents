#'indiv_info_merge
#'
#'Merges two individual_info dataframes
#'
#'@param indiv_infoA first dataframe
#'@param indiv_infoB second dataframe
#'
#'@return merged dataframe
#'
#'@export

indiv_info_merge<-function(indiv_infoA,indiv_infoB){
  indiv_info_comb<-merge(indiv_infoA,indiv_infoB,by="ID")
  return(indiv_info_comb)
}
